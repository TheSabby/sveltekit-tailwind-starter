{
	"root": true,
	"plugins": ["prettier", "svelte3", "security"],
	"extends": ["eslint:recommended", "plugin:security/recommended"],
	"overrides": [{ "files": ["*.svelte"], "processor": "svelte3/svelte3" }],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020
	},
	"env": {
		"browser": true,
		"es2017": true,
		"node": true
	}
}
